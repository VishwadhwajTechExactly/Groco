@model Category

<h1 style="margin:100px" class="text-center">
	Delete Category
</h1>

<form method="post" enctype="multipart/form-data">
	<div style="margin:20px">
		<label asp-for="Name"></label>
		<input type="text" disabled asp-for="Name" />
	</div>
	<div style="margin:20px">
		<label asp-for="Discount"></label>
		<input type="text" disabled asp-for="Discount" />
	</div>
	<div style="margin:20px">
		<input type="file" disabled name="file" asp-for="Discount" />
	</div>
	<div style="margin:20px">
		<img src="@Model.ImageUrl" />
	</div>
	<div style="margin:20px">
		<button type="submit" class="btn btn-primary">Delete</button>
	</div>
</form>